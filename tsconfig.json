{
  "compilerOptions": {
    "module": "CommonJS",
    "esModuleInterop": true,
    "target": "es6",
    "rootDir": "./",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "Node",
    "noImplicitThis": true,
    "noUnusedLocals": true,
  },
  "exclude": [
    "dist",
    "node_modules",
  ]
}